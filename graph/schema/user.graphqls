# The Information available on users for all.
type User {
    id: ID!
    name: String!
    username: String!
}

# The Input for updateing a user.
input UpdateUserInput {
    name: String!
    username: String!
}

# The Input for creating a new user.
input CreateUserInput {
    name: String!
    username: String!
    password: String!
}