type Sensor implements Node {
    id: ID!
    name: String!
    description: String!
    transform_code: String!
    raw_value: Float!
    value: Float!
}

input SensorInput {
    id: Int!
    name: String!
    description: String!
    transform_code: String!
}

input SensorQuery {
    raw: Boolean!
    id: ID!
}